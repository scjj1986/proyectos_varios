                            <Label Content="*Cédula:" HorizontalAlignment="Left" Margin="15,36,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="26" Width="56"/>
                            <ComboBox x:Name="cmbNacCom" HorizontalAlignment="Left" Margin="86,36,0,0" VerticalAlignment="Top" Width="56" />
                            <TextBox x:Name="txtCedCom" HorizontalAlignment="Left" Height="26" Margin="142,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" LostFocus="txtCedCom_LostFocus" PreviewTextInput="NumberValidationTextBox"/>

                            <Label Content="*R.I.F.:" HorizontalAlignment="Left" Margin="315,36,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="26" Width="47"/>
                            <ComboBox x:Name="cmbNacRIFCom" HorizontalAlignment="Left" Margin="368,36,0,0" VerticalAlignment="Top" Width="56" />
                            <TextBox x:Name="txtRIFCom" HorizontalAlignment="Left" Height="26" Margin="424,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" PreviewTextInput="NumberValidationTextBox"/>

                            <Label Content="*Nombre Completo:" Margin="587,36,583,0" VerticalAlignment="Top" FontWeight="Bold" Height="26"/>
                            <TextBox x:Name="txtNomCom" HorizontalAlignment="Left" Height="26" Margin="716,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="268"/>

                            <Label Content="*Duración:" Margin="10,91,1215,0" VerticalAlignment="Top" FontWeight="Bold" Height="26"/>
                            <TextBox x:Name="txtDurCom" HorizontalAlignment="Left" Height="26" Margin="84,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115"/>


                            <Label Content="*CPC:" Margin="321,91,931,0" VerticalAlignment="Top" FontWeight="Bold" Height="26"/>
                            <TextBox x:Name="txtCPCCom" HorizontalAlignment="Left" Height="26" Margin="368,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115"/>


                        </Grid>
                    </TabItem>

                    <TabItem x:Name="tbActReg" Header="Actas Registradas">
                        <Grid Background="#FFE5E5E5" Margin="0,5,10,0" Height="404" VerticalAlignment="Top">

                            <DatePicker x:Name="dpFechaCelebActReg" HorizontalAlignment="Left" Margin="145,20,0,0" VerticalAlignment="Top" IsEnabled="True" FontSize="10"/>
                            <Label Content="*Fecha Celebración:" Margin="18,22,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Width="122" RenderTransformOrigin="0.8,0.583"/>

                            <DatePicker x:Name="dpFechaProtActReg" HorizontalAlignment="Left" Margin="431,18,0,0" VerticalAlignment="Top" IsEnabled="True" FontSize="10"/>
                            <Label Content="*Fecha Protocolización:" Margin="285,20,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Width="141" RenderTransformOrigin="0.8,0.583"/>

                            <Label Content="*N°:" Margin="109,72,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Width="35" RenderTransformOrigin="0.8,0.583"/>
                            <TextBox x:Name="txtNrActReg" HorizontalAlignment="Left" Height="26" Margin="144,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>

                            <Label Content="*Puntos a Tratar:" Margin="603,19,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="27" HorizontalAlignment="Left" Width="106" RenderTransformOrigin="0.8,0.583"/>
                            <TextBox x:Name="txtPtosATratActReg" HorizontalAlignment="Left" Height="66" Margin="714,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="184"  />


                            <Label Content="*Tomo:" Margin="373,72,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Width="56" RenderTransformOrigin="0.8,0.583"/>
                            <TextBox x:Name="txtTomoActReg" HorizontalAlignment="Left" Height="26" Margin="429,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="77"/>

                            <Label Content="Búsqueda:" HorizontalAlignment="Left" Margin="72,138,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="26" Width="68" RenderTransformOrigin="0.484,0.462"/>
                            <TextBox x:Name="txtBsqActReg" HorizontalAlignment="Left" Height="26" Margin="141,138,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" KeyUp="txtBsqActReg_KeyUp"/>


                            <Button ToolTip="Guardar" x:Name="btnGuardarActReg" Content="" HorizontalAlignment="Left" Margin="847,138,0,0" VerticalAlignment="Top" Width="51" Height="45" Grid.Column="0" RenderTransformOrigin="0.5,0.76" Click="btnGuardarActReg_Click">
                                <Button.Background>
                                    <ImageBrush ImageSource="Images/1353183085_save.png" Stretch="Uniform"/>
                                </Button.Background>
                            </Button>

                            <Button ToolTip="Nuevo" x:Name="btnNuevoActReg" Content="" HorizontalAlignment="Left" Margin="787,138,0,0" VerticalAlignment="Top" Width="55" Height="45" Grid.Column="0" RenderTransformOrigin="0.5,0.76" Click="btnNuevoActReg_Click">
                                <Button.Background>
                                    <ImageBrush ImageSource="Images/add.png" Stretch="Uniform"/>
                                </Button.Background>
                            </Button>

                            <DataGrid x:Name="dtgActReg" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="888" Height="185" AutoGenerateColumns="False" CanUserAddRows="False" CanUserDeleteRows="False" MouseDoubleClick="dtgActReg_MouseDoubleClick">
                                <DataGrid.Columns>


                                    <DataGridTextColumn Visibility="Hidden" Binding="{Binding ptosatratar, Mode=OneWay}"  CanUserResize="False" IsReadOnly="True" Header="rif">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>

                                    <DataGridTextColumn Visibility="Hidden" Binding="{Binding fcel, Mode=OneWay}"  CanUserResize="False" IsReadOnly="True" Header="rif">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>

                                    <DataGridTextColumn Visibility="Hidden" Binding="{Binding fprot, Mode=OneWay}"  CanUserResize="False" IsReadOnly="True" Header="rif">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>



                                    <DataGridTextColumn Width="120" Binding="{Binding nr, Mode=TwoWay}"  CanUserResize="False" IsReadOnly="True" Header="Número">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>

                                    <DataGridTextColumn Width="120" Binding="{Binding tomo, Mode=TwoWay}"  CanUserResize="False" IsReadOnly="True" Header="tomo">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>

                                    <DataGridTextColumn Width="220" Binding="{Binding string_fcel, Mode=TwoWay}"  CanUserResize="False" IsReadOnly="True" Header="Fecha celebración">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>

                                    <DataGridTextColumn Width="220" Binding="{Binding string_fprot, Mode=TwoWay}"  CanUserResize="False" IsReadOnly="True" Header="Fecha protocolización">
                                        <DataGridTextColumn.CellStyle>
                                            <Style>
                                                <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left"/>
                                            </Style>
                                        </DataGridTextColumn.CellStyle>
                                    </DataGridTextColumn>




                                </DataGrid.Columns>
                                <DataGrid.Resources>
                                    <ContextMenu x:Key="RowMenu" 
            DataContext="{Binding PlacementTarget.DataContext, RelativeSource={RelativeSource Self}}">
                                        <MenuItem x:Name="ElimFildtgActReg" Header="Eliminar de la Lista" Click="ElimFildtgActReg_Click" />
                                        <MenuItem x:Name="ElimTododtgActReg" Header="Eliminar Todo" Click="ElimTododtgActReg_Click" />
                                    </ContextMenu>
                                </DataGrid.Resources>
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow" >
                                        <Setter Property="ContextMenu" Value="{StaticResource RowMenu}" />
                                    </Style>
                                </DataGrid.RowStyle>


                            </DataGrid>


                        </Grid>
                    </TabItem>



                </TabControl>

                <Button ToolTip="Nuevo" x:Name="btnNuevoEmp" Content="" Margin="1172,831,0,0" Grid.Column="0" RenderTransformOrigin="0.646,1.263" Click="btnNuevoEmp_Click" HorizontalAlignment="Left" Width="65" Height="57" VerticalAlignment="Top">
                    <Button.Background>
                        <ImageBrush ImageSource="Images/add.png" Stretch="Uniform"/>
                    </Button.Background>
                </Button>

                <Button ToolTip="Guardar" x:Name="btnGuardarEmp" Content="" HorizontalAlignment="Left" Margin="1242,830,0,0" VerticalAlignment="Top" Width="68" Height="57" Grid.Column="0" Click="btnGuardarEmp_Click">
                    <Button.Background>
                        <ImageBrush ImageSource="Images/1353183085_save.png" Stretch="Uniform"/>
                    </Button.Background>
                </Button>






            </Grid>
        </ScrollViewer>
    </GroupBox>
</Page>
